{
    "parserOptions": {
        "ecmaVersion": 9,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "node": true
    },
    "globals": {
        "Map": true,
        "Promise": true
    },
    "rules": {
        "eqeqeq": "warn",
        "semi": "warn",
        "curly": "warn",
        "no-empty": "warn",
        "valid-jsdoc": "warn",
        "no-extra-semi": "warn",
        "no-unused-vars": "warn",
        "no-undef": "error"
    }
}